<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Consumption" table="consumption">
		<id name="id" type="int">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="member_id" type="int">
			<column name="member_id" />
		</property>
		<property name="movie_id" type="int">
			<column name="movie_id" />
		</property>
		<property name="seat" type="int">
			<column name="seat" />
		</property>
		<property name="way" type="java.lang.String">
			<column name="way" />
		</property>
		<property name="movie_name" type="java.lang.String">
			<column name="movie_name" />
		</property>
		<property name="movie_time" type="java.lang.String">
			<column name="movie_time" />
		</property>
		<property name="movie_hall" type="int">
			<column name="movie_hall" />
		</property>
	</class>
</hibernate-mapping>